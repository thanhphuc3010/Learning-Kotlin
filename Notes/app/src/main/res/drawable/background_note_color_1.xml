<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <stroke
                android:width="4dp"
                android:color="@color/color_default_note" />
        </shape>
    </item>

    <item
        android:bottom="4dp"
        android:left="4dp"
        android:right="4dp"
        android:top="4dp">

        <shape android:shape="oval">
            <stroke
                android:width="4dp"
                android:color="@color/color_primary" />
        </shape>
    </item>

    <item
        android:bottom="8dp"
        android:left="8dp"
        android:right="8dp"
        android:top="8dp">

        <shape android:shape="oval">
            <solid android:color="@color/color_default_note" />
        </shape>
    </item>

</layer-list>